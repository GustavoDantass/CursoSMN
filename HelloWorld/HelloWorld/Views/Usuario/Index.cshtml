<div class="container" id="divContainerUsuario">
    <h4>
       Usuario
    </h4>
    <div class="row">
        <div class="col s1 offset-s10">
            <span class="flow-text">
                <a class="btn-floating btn-large waves-effect waves-light green" title="Novo" onclick="abrirCadastroLancamento()"><i class="material-icons">add</i></a>
            </span>
        </div>
    </div>
    <div id="divGridUsuario">
        @*@Html.Partial("_Grid")*@
    </div>
</div>

<script>

    function buscarGrid() {
        $.get('@Url.Action("BuscarGrid", "Usuario")').done(function (data) {
            $("#divGridUsuario").html(data);
        });
    }

    buscarGrid();

</script>